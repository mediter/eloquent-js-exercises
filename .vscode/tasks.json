{
  "version": "2.0.0",
  "command": "node",
  "type": "shell",
  "args": [
    "--harmony"
  ],

  "tasks": [{
    "label": "runFile",
    "presentation": {
      "echo": true,
      "reveal": "always",
      "focus": false,
      "panel": "shared",
      "showReuseMessage": true,
      "clear": false
    },
    "group": "test",
    "problemMatcher": "$jshint",
    "args": ["${file}"]
  }]
}